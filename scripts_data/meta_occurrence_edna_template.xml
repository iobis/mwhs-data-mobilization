<?xml version="1.0" encoding="UTF-8"?>
<archive xmlns="http://rs.tdwg.org/dwc/text/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://rs.tdwg.org/dwc/text/ http://rs.tdwg.org/dwc/text/tdwg_dwc_text.xsd">
    <core rowType="http://rs.tdwg.org/dwc/terms/Occurrence" ignoreHeaderLines="1">
	<files>
	    <location>occurrence.csv</location>
    	</files>
    	<coreid index="0"/>
    	<field index="1" term="http://rs.tdwg.org/dwc/terms/scientificName" />
    	<field index="2" term="http://rs.tdwg.org/dwc/terms/scientificNameID" />
        <field term="http://rs.tdwg.org/dwc/terms/eventDate" default="###"/>
        <field term="http://rs.tdwg.org/dwc/terms/country" default="###"/>
        <field index="5" term="http://rs.tdwg.org/dwc/terms/locality" />
        <field index="7" term="http://rs.tdwg.org/dwc/terms/decimalLatitude" />
        <field index="8" term="http://rs.tdwg.org/dwc/terms/decimalLongitude" />
        <field term="http://rs.tdwg.org/dwc/terms/geodeticDatum" default="###"/>
        <field term="http://rs.tdwg.org/dwc/terms/occurrenceStatus" default="###"/>
        <field term="http://rs.tdwg.org/dwc/terms/basisOfRecord" default="###"/>
    </core>
  <extension ignoreHeaderLines="1" rowType="http://rs.gbif.org/terms/1.0/DNADerivedData">
    <files>
      <location>dnaderiveddata.csv</location>
    </files>
    <coreid index="0"/>
    <field index="1" term="https://w3id.org/gensc/terms/MIXS:0000012"/>
    <field index="2" term="https://w3id.org/gensc/terms/MIXS:0000041"/>
    <field index="3" term="https://w3id.org/gensc/terms/MIXS:0000044"/>
    <field index="4" term="https://w3id.org/gensc/terms/MIXS:0000050"/>
    <field index="5" term="http://rs.gbif.org/terms/pcr_primer_name_forward"/>
    <field index="6" term="http://rs.gbif.org/terms/pcr_primer_name_reverse"/>
    <field index="7" term="http://rs.gbif.org/terms/pcr_primer_reference"/>
    <field index="8" term="http://rs.gbif.org/terms/dna_sequence"/>
    <field index="9" term="http://rs.gbif.org/terms/pcr_primer_forward"/>
    <field index="10" term="http://rs.gbif.org/terms/pcr_primer_reverse"/>
  </extension>
</archive>